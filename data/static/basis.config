{
  "create": {
    "appName": "app",
    "path": {
      "type": "src/app/type",
      "typeIndex": "src/app/type.js",
      "module": "src/module"
    }
  },
  "build": {
    "file": "index.html",
    "output": "build"
  },
  "server": {
    "port": 8001,
    "rewrite": {
      "^/api/(.*)$": "http://api.eventshot.me/$1 [P, QSA]"
    }
  }
}